<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="ventana">
    <property name="width_request">400</property>
    <property name="height_request">300</property>
    <property name="window_position">center</property>
    <child>
      <object class="GtkFixed" id="fx">
        <property name="width_request">400</property>
        <property name="height_request">300</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkButton" id="b_new">
            <property name="label" translatable="yes">Cambiar Tipo</property>
            <property name="width_request">100</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="x">25</property>
            <property name="y">250</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b_menu">
            <property name="label" translatable="yes">Menu Principal</property>
            <property name="width_request">100</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="x">325</property>
            <property name="y">250</property>
          </packing>
        </child>
        <child>
          <object class="GtkImage" id="player">
            <property name="width_request">120</property>
            <property name="height_request">75</property>
            <property name="visible">True</property>
            <property name="pixbuf">pares_y_nones/sprites/1.png</property>
          </object>
          <packing>
            <property name="x">50</property>
            <property name="y">100</property>
          </packing>
        </child>
        <child>
          <object class="GtkImage" id="bot">
            <property name="width_request">120</property>
            <property name="height_request">75</property>
            <property name="visible">True</property>
            <property name="pixbuf">pares_y_nones/sprites/2.png</property>
          </object>
          <packing>
            <property name="x">280</property>
            <property name="y">100</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b1">
            <property name="width_request">20</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="x">100</property>
            <property name="y">200</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b2">
            <property name="width_request">20</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="x">330</property>
            <property name="y">200</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="l_wins">
            <property name="width_request">150</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes"> </property>
          </object>
          <packing>
            <property name="x">140</property>
            <property name="y">256</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
