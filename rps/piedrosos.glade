<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="ventana">
    <property name="width_request">450</property>
    <property name="height_request">300</property>
    <property name="resizable">False</property>
    <property name="window_position">center</property>
    <child>
      <object class="GtkFixed" id="fx">
        <property name="width_request">450</property>
        <property name="height_request">350</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkImage" id="img_logo">
            <property name="width_request">400</property>
            <property name="height_request">125</property>
            <property name="visible">True</property>
            <property name="pixbuf">rps/sprites/logo.png</property>
          </object>
          <packing>
            <property name="x">25</property>
            <property name="y">25</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b_s">
            <property name="width_request">100</property>
            <property name="height_request">100</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <child>
              <object class="GtkImage" id="s">
                <property name="visible">True</property>
                <property name="pixbuf">rps/sprites/s.png</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="x">325</property>
            <property name="y">175</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b_p">
            <property name="width_request">100</property>
            <property name="height_request">100</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <child>
              <object class="GtkImage" id="p">
                <property name="visible">True</property>
                <property name="pixbuf">rps/sprites/p.png</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="x">175</property>
            <property name="y">175</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b_r">
            <property name="width_request">100</property>
            <property name="height_request">100</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <child>
              <object class="GtkImage" id="r">
                <property name="visible">True</property>
                <property name="pixbuf">rps/sprites/r.png</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="x">25</property>
            <property name="y">175</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="a">
            <property name="width_request">20</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes"> </property>
          </object>
          <packing>
            <property name="x">73</property>
            <property name="y">155</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="b_menu">
            <property name="label" translatable="yes">MENU</property>
            <property name="width_request">100</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
          <packing>
            <property name="x">330</property>
            <property name="y">300</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="l_wins">
            <property name="width_request">200</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">Wins: </property>
            <property name="ellipsize">start</property>
          </object>
          <packing>
            <property name="x">100</property>
            <property name="y">300</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
